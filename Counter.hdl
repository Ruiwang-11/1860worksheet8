CHIP Counter {
    IN inc, reset;
    OUT out0, out1, out2, out3, out4, out5, out6, out7,
        out8, out9, out10, out11, out12, out13, out14, out15;

    PARTS:
    Inc16(in=registerout, out=intomux);
    Mux16(a=intomux, b=outmux2, sel=inc, out=outmux);
    Mux16(a=intomux, b=false, sel=reset, out=outmux2);
    Register(in=outmux2, load=true, out=registerout);
}
