CHIP DisplayCounter201890224 {
    IN inc, reset;
    OUT a, b, c, d, e, f, g;

    PARTS:
    // 调用 Counter 芯片，并把它的输出赋值给中间变量 w0 ~ w3
    Counter(inc=inc, reset=reset,
            out0=w0, out1=w1, out2=w2, out3=w3);

    // 调用你的七段译码器，传入 w0 ~ w3
    Decoder201890224(
        A=w0,
        B=w1,
        C=w2,
        D=w3,
        a=a, b=b, c=c, d=d, e=e, f=f, g=g
    );
}
